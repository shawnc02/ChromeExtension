{
    "manifest_version": 3,
    "name": "Candle AI",
    "icons": {
        "32": "candle.jpeg"
    },    
    "version": "1.0.0",
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    },
    "action": { 
        "default_popup": "index.html"
    },
    
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
